<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="383.0" prefWidth="318.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.RemoveMemberController">
   <children>
      <Label layoutX="48.0" layoutY="55.0" text="Select staff member to remove:" textFill="#797979">
         <font>
            <Font name="Microsoft Sans Serif" size="16.0" />
         </font>
      </Label>
      <Button layoutX="97.0" layoutY="222.0" mnemonicParsing="false" onAction="#clickRemove" prefHeight="42.0" prefWidth="173.0" style="-fx-background-color: rgb(15,157,88);" text="Remove" textFill="WHITE">
         <font>
            <Font name="Microsoft Sans Serif" size="18.0" />
         </font>
      </Button>
      <Button layoutX="48.0" layoutY="224.0" mnemonicParsing="false" onAction="#clickBack" style="-fx-background-color: transparent;">
         <graphic>
            <FontAwesomeIconView fill="#0f9d58" glyphName="ARROW_LEFT" size="30" />
         </graphic>
      </Button>
      <ComboBox fx:id="removeCombo" layoutX="48.0" layoutY="132.0" prefHeight="25.0" prefWidth="222.0" />
   </children>
</AnchorPane>
